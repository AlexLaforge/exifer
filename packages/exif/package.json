{
  "name": "@exifer/exif",
  "version": "1.0.0-beta.2",
  "repository": "terkelg/exifer",
  "description": "Exif tags and parsers",
  "main": "dist/exifer-exif.js",
  "module": "dist/exifer-exif.mjs",
  "browser": "dist/exifer-exif.umd.js",
  "unpkg": "dist/exifer-exif.umd.js",
  "files": [
